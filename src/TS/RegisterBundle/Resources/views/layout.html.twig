{% extends "@App//layout.html.twig" %}

{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('bundles/tsregister/css/style.css') }}" type="text/css" media="all" />
{% endblock %}

{% block body %}	
    <Br/>
    {% for message in app.session.flashbag.get('notice') %}
        <div class="alert alert-info">Message flash : {{ message }}</div>
    {% endfor %}
{% endblock %}

{% block javascript %}
    <script src="{{ asset('bundles/tsregister/js/register.js') }}"></script>
{% endblock %}