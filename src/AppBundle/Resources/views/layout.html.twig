<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <title>{% block title %}Omega.PV{% endblock %}</title>
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
		<link rel="stylesheet" href="{{ asset('bundles/app/css/styleLayout.css') }}" type="text/css" media="all" />
		{% block stylesheets %}
		{% endblock %}
    </head>

    <body>
		<header>
			<div id="titre_principal">
				<a href="{{ path('ts_homepage_home')}}">
					<div id="vignette">
						<img src="{{ asset('bundles/app/images/LogoDarkGrey.png') }}" alt="Logo TS" id="logo" />
						<h1 id="titre">MEGA.PV</h1>
					</div>
				</a>
				<div id="menu">
					<nav>
						<ul class="nav nav-inline">
							<li role="presentation"><a href="{{ path('ts_register_homepage')}}">Register</a></li>
							<li role="presentation"><a href="{{ path('ts_assets_homepage')}}">Assets</a></li>
							<li role="presentation" class="dropdown"><a class="dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Data & analysis <span class="caret"></span></a>
								<ul class="dropdown-menu">
									<li><a href="{{ path('ts_data_manager_import')}}">Import</a></li>
									<li><a href="{{ path('ts_data_manager_homepage')}}">Manage</a></li>
									<li><a href="{{ path('ts_data_manager_curves')}}">Graph & analysis</a></li>
								</ul>
							</li>
							<li role="presentation"><a href="{{ path('ts_reports_homepage')}}">Reports</a></li>
						</ul>
					</nav>
				</div>
			</div>
		</header>

		{% block body %}
		{% endblock %}

		<script src="{{ asset('bundles/app/js/ajax.js') }}"></script>
		<script src="{{ asset('bundles/app/js/jquery-3.3.1.slim.min.js')}}"></script>
		<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
		
		{% block javascript %}
		{% endblock %}
    </body>
	
</html>